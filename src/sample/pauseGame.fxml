<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pauseGameRoot" prefHeight="600.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.pauseController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/GamePaused.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="Exit" fitHeight="40.0" fitWidth="453.0" layoutX="278.0" layoutY="360.0" onMouseClicked="#ExitToMainMenu" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="SaveGame" fitHeight="40.0" fitWidth="453.0" layoutX="280.0" layoutY="293.0" pickOnBounds="true"  preserveRatio="true" />
      <ImageView fx:id="ResumeGame" fitHeight="49.0" fitWidth="462.0" layoutX="279.0" layoutY="227.0" onMouseClicked="#resumeGame" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
